<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.association.mapper.studentMapper2">
    <select id="getstudentbyid" resultType="com.association.emtity.student">
        select * from student where studentID=#{id}
    </select>
    <select id="older" resultType="com.association.emtity.student">
        select * from student where studentGrade<![CDATA[ >= ]]>#{grade}
    </select>

</mapper>